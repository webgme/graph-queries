{
  "id": "GraphDBExporter",
  "name": "Graph Database Exporter",
  "version": "0.1.0",
  "description": "Maps the model to a OrientDB database",
  "icon": {
    "class": "fa fa-database",
    "src": ""
  },
  "disableServerSideExecution": false,
  "disableBrowserSideExecution": true,
  "writeAccessRequired": false,
  "configStructure": [
    {
      "name": "host",
      "displayName": "Host",
      "description": "OrientDB host.",
      "value": "localhost",
      "valueType": "string",
      "readOnly": false
    },
    {
      "name": "port",
      "displayName": "Port",
      "description": "Binary OrientDB port.",
      "value": 2424,
      "valueType": "number",
      "minValue": 0,
      "readOnly": false
    },
    {
      "name": "username",
      "displayName": "Username",
      "description": "Username for OrientDB.",
      "value": "root",
      "valueType": "string",
      "readOnly": false
    },
    {
      "name": "password",
      "displayName": "Password",
      "description": "Password for username in OrientDB.",
      "value": "root",
      "valueType": "string",
      "readOnly": false
    },
    {
      "name": "batchSize",
      "displayName": "Batch sizes",
      "description": "Max number of parallel insert requests to the graphDB",
      "value": 1000,
      "valueType": "number",
      "minValue": 1,
      "readOnly": false
    }
  ]
}
